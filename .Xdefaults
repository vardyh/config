xft*dpi: 96
Xft*antialias: true
Xft*hintstyle: hintsnone
Xft*smoothing: subpixel

urxvt*title: rxvt-unicode
urxvt*termName: xterm
urxvt*iso14755: false
urxvt*cursorBlink: true
urxvt*geometry: 160x39
urxvt*font: xft:profont:pixelsize=9,xft:HYXiZhongYuanJ:pixelsize=9:bold
urxvt*boldFont: xft:profont:pixelsize=9,xft:HYXiZhongYuanJ:pixelsize=9:bold
urxvt*scrollBar: false
urxvt*tintColor: #306030
urxvt*shading: 5
urxvt*visualBell: false
urxvt*urlLauncher: open

urxvt*backgroundPixmap: /Users/vardyh/.config/pics/termbk.jpg
urxvt*background: #222222
urxvt*foreground: #dddddd

! black
