use_xft yes
xftfont Segoe UI:size=8
alignment bottom_left 

update_interval 1
cpu_avg_samples 2
net_avg_samples 2
total_run_times 0

minimum_size 1280 0
maximum_width 1280

own_window yes
own_window_type panel
alignment top_left

draw_shades no
double_buffer yes

default_color white
own_window_colour #333333

#own_window_transparent yes
#own_window_hints undecorated,below,sticky,skip_taskbar,skip_pager

gap_x 0
gap_y 0

TEXT
cpu: ${cpu cpu1}%, ${cpu cpu2}% | memory: $memperc% | uptime: ${uptime} | down: ${downspeed wlan0} ${totaldown wlan0} downloaded | up: ${upspeed wlan0} ${totalup wlan0} uploaded | /: ${fs_free /}/${fs_size /} free | /home: ${fs_free /home}/${fs_size /home} free | battery: ${battery} ${battery_time} | mixer: ${mixerl}/${mixer}${alignr}${time}
